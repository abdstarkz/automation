{
  "name": "admp",
  "version": "1.0.0",
  "description": "AI-powered health and productivity management platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx --tsconfig tsconfig.seed.json prisma/seed.ts",
    "worker": "tsx --tsconfig tsconfig.server.json workers/worker.ts",
    "dev:worker": "node dist/workers/worker.js",
    "server": "tsx watch server/server.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "dev:server": "node dist/server/server.js",
    "clean": "rd /s /q dist",
    "clean:all": "rd /s /q dist & rd /s /q node_modules/.vite",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "fix-dates": "ts-node --project tsconfig.server.json scripts/fix-dates.ts",
    "est-mac": "ts-node --project tsconfig.server.json scripts/estimate-missing-macros.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run build:server\" \"npm run dev:worker\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.24.1",
    "@notionhq/client": "^5.4.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.57.4",
    "@types/node": "^24.9.1",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "axios": "^1.13.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "bullmq": "^5.28.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron-parser": "^5.4.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.22.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "framer-motion": "^12.23.24",
    "googleapis": "^166.0.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.344.0",
    "nanoid": "^5.0.9",
    "node-cron": "^4.2.1",
    "oauth-1.0a": "^2.2.6",
    "openai": "^6.7.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.11.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^7.9.5",
    "reactflow": "^11.11.4",
    "recharts": "^3.4.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "twilio": "^5.10.7",
    "uuid": "^11.0.3",
    "zod": "^3.24.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "tsc-esm-fix": "^3.1.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
